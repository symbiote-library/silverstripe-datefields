{
    "name": "symbiote-library/symbiote-datefields",
    "description": "Adds better date fields for CMS users",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Stephen McMahon",
            "email": "stephen@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\datefields\\": "src/",
            "Symbiote\\datefields\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        },
        "expose": [
          "client/javascript",
          "client/css"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
